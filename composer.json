{
    "name": "simplesamlphp/simplesamlphp-module-authlinkedin",
    "description": "A module that is able to authenticate against LinkedIn",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "linkedin"],
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Olav Morken",
            "email": "olavmrk@gmail.com"
        },
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\authlinkedin\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": ">=5.6",
        "simplesamlphp/composer-module-installer": "~1.1"
        "simplesamlphp/composer-module-oauth": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "simplesamlphp/simplesamlphp": "^1.17",
        "webmozart/assert": "^1.4"
    },
    "support": {
        "issues": "https://github.com/tvdijen/simplesamlphp-module-authlinkedin/issues",
        "source": "https://github.com/tvdijen/simplesamlphp-module-authlinkedin"
    }
}
